{
  "name": "YouSlow Kell",
  "description": "",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "https://www.youtube.com/*",
    "https://www.netflix.com/*",
    "http://localhost:34543/"
  ],
  "content_scripts": [{
    "matches": [
      "https://www.youtube.com/*",
      "https://www.netflix.com/*"
    ],
    "js": ["content.js"],
    "run_at": "document_end"
  }],
  "web_accessible_resources": [{
    "resources": ["youtube_script.js","netflix_script.js"],
    "matches": ["<all_urls>"]
  }]
}